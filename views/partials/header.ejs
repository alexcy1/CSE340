<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (locals.loggedin && locals.accountData) { %>
      <a title="Click to manage your account" href="/account/">
        Welcome <%= locals.accountData.account_firstname %>
      </a>
      | <a class="logout" title="Click to log out" href="/account/logout">
        <i class="fas fa-sign-out-alt"></i> Logout
      </a>
      <!-- <% if (locals.accountData.account_type === 'Admin') { %>
        | <a href="/account/manage-users">Manage Users</a>
      <% } %> -->
    <% } else { %>
      <a class="login" title="Click to log in" href="/account/login">
        <i class="fas fa-user"></i> Login
      </a>
    <% } %>
  </div>
</header>
